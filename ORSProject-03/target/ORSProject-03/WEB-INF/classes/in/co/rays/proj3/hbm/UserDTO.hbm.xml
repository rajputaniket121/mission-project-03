<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="in.co.rays.proj3.dto.UserDTO" table="ST_USER">
		<id name="id" type="java.lang.Long">
			<column name="ID"></column>
			<generator class="increment"></generator>
		</id>
		
		<property name="firstName" type="java.lang.String">
			<column name="FIRST_NAME" />
		</property>
		<property name="lastName" type="java.lang.String">
			<column name="LAST_NAME" />
		</property>
		<property name="gender" type="java.lang.String">
			<column name="GENDER" />
		</property>
		<property name="roleId" type="integer">
			<column name="ROLE_ID" />
		</property>
		<property name="dob" type="java.util.Date">
			<column name="DOB" />
		</property>

		<property name="mobileNo" type="java.lang.String">
			<column name="MOBILE_NO" />
		</property>
		<property name="login" type="java.lang.String">
			<column name="LOGIN" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="PASSWORD" />
		</property>
		<property name="createdBy" type="java.lang.String" length="50">
			<column name="CREATED_BY" />
		</property>
		<property name="modifiedBy" type="java.lang.String" length="50">
			<column name="MODIFIED_BY" />
		</property>
		<property name="createdDateTime" type="java.sql.Timestamp">
			<column name="CREATED_DATETIME" />
		</property>
		<property name="modifiedDateTime" type="java.sql.Timestamp">
			<column name="MODIFIED_DATETIME" />
		</property>
	</class>
</hibernate-mapping>
