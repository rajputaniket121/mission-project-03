<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="in.co.rays.proj3.dto.ContactDTO" table="ST_CONTACT">
        <id name="id" type="java.lang.Long">
            <column name="ID"></column>
            <generator class="increment"></generator>
        </id>
        <property name="name" type="java.lang.String">
            <column name="NAME" />
        </property>
        <property name="email" type="java.lang.String">
            <column name="EMAIL" />
        </property>
        <property name="mobileNo" type="java.lang.String">
            <column name="MOBILE_NO" />
        </property>
        <property name="message" type="java.lang.String">
            <column name="MESSAGE" />
        </property>
        <property name="createdBy" type="java.lang.String" length="50">
            <column name="CREATED_BY" />
        </property>
        <property name="modifiedBy" type="java.lang.String" length="50">
            <column name="MODIFIED_BY" />
        </property>
        <property name="createdDateTime" type="java.sql.Timestamp">
            <column name="CREATED_DATETIME" />
        </property>
        <property name="modifiedDateTime" type="java.sql.Timestamp">
            <column name="MODIFIED_DATETIME" />
        </property>
    </class>
</hibernate-mapping>