<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="in.co.rays.proj3.dto.AttendanceDTO" table="ST_ATTENDANCE">
        <id name="id" type="java.lang.Long">
            <column name="ID"></column>
            <generator class="increment"></generator>
        </id>
        <property name="personName" type="java.lang.String">
            <column name="PERSON_NAME" />
        </property>
        <property name="attendanceDate" type="java.util.Date">
            <column name="ATTENDANCE_DATE" />
        </property>
        <property name="attendanceStatus" type="java.lang.String">
            <column name="ATTENDANCE_STATUS" />
        </property>
        <property name="remarks" type="java.lang.String">
            <column name="REMARKS" />
        </property>
        <property name="createdBy" type="java.lang.String" length="50">
            <column name="CREATED_BY" />
        </property>
        <property name="modifiedBy" type="java.lang.String" length="50">
            <column name="MODIFIED_BY" />
        </property>
        <property name="createdDateTime" type="java.sql.Timestamp">
            <column name="CREATED_DATETIME" />
        </property>
        <property name="modifiedDateTime" type="java.sql.Timestamp">
            <column name="MODIFIED_DATETIME" />
        </property>
    </class>
</hibernate-mapping>